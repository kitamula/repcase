{
  "name": "repcase",
  "publisher": "kitamula",
  "displayName": "Repcase",
  "description": "Save and load replace rules.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:repcase.helloWorld",
    "onCommand:repcase.commonToAsset"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "repcase.helloWorld",
        "title": "Hello World",
        "category": "Repcase"
      },
      {
        "command": "repcase.commonToAsset",
        "title": "Replace common to asset",
        "category": "Repcase"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "vsce": "^2.15.0"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/kitamula/repcase.git"
  }
}
